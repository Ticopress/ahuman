<service>
	<listeners>
		<listener name="debug" type="tcp">
			<property name="port" value="20001"/> 
			<property name="topic.in" value="debug"/> 
		</listener>
		<listener name="chat" type="tcp">
			<property name="port" value="20002"/> 
			<property name="topic.in" value="chat.in"/> 
			<property name="topic.out" value="chat.out"/> 
		</listener>
	</listeners>
</service>
