<engine>
	<!-- service configuration -->

	<services>
		<service name="AIMedia">
			<property name="file" value="media.xml"/>
		</service>
		<service name="AIIO">
			<property name="file" value="io.xml"/>
		</service>
		<service name="AIHeart">
			<property name="file" value="heart.xml"/>
		</service>
		<service name="AIDB">
			<property name="file" value="database.xml"/>
		</service>
		<service name="AIKnowledge">
			<property name="file" value="knowledge.xml"/>
		</service>
		<service name="AIMind">
			<property name="file" value="mind.xml"/>
		</service>
		<service name="AINNLib">
			<property name="file" value="nnlib.xml"/>
		</service>
		<service name="AIHtmLib">
			<property name="file" value="htmlib.xml"/>
		</service>
	</services>

	<!-- engine configuration -->

	<logging>
		<property name="filename" value="K:\projects\ai2svn\environments\local\log\main.log"/>
		<property name="format" value="log: %"/>
		
		<objectLogLevel default="E">
			<class name="NNFinderFactory" level="A"/>
			<class name="NNFinder" level="A"/>
		</objectLogLevel>

		<objectInstanceLogLevel default="E">
			<class name="NNStrategyBackPropagation" instance = "NNFinderFactory::P1S" level="E"/>
		</objectInstanceLogLevel>

		<serviceLogLevel default="A">
		</serviceLogLevel>

		<customLogLevel default="E">
			<class name="SocketServer::debug" level="A"/>
			<class name="SocketServer::chat" level="A"/>
			<class name="SocketConnection" level="A"/>
		</customLogLevel>
	</logging>

	<debug>
		<property name="debugchannel" value="debug.commands"/>
	</debug>
</engine>
