<engine>
	<!-- service configuration -->

	<services>
		<service name="AIMedia">
			<property name="file" value="media.xml"/>
		</service>
		<service name="AIIO">
			<property name="file" value="io.xml"/>
		</service>
		<service name="AIHeart">
			<property name="file" value="heart.xml"/>
		</service>
		<service name="AIDB">
			<property name="file" value="database.xml"/>
		</service>
		<service name="AIKnowledge">
			<property name="file" value="knowledge.xml"/>
		</service>
		<service name="AIMind">
			<property name="file" value="mind.xml"/>
		</service>
		<service name="AIExpert">
			<property name="file" value="expert.xml"/>
		</service>
	</services>

	<!-- engine configuration -->
	<!-- logging -->

	<logging>
		<property name="filename" value="log/main.log"/>
		<property name="format" value="log: %"/>
		
		<objectLogLevel default="N">
			<class name="NNFinderFactory" level="A"/>
			<class name="NNFinder" level="A"/>
		</objectLogLevel>

		<objectInstanceLogLevel default="N">
			<class name="NNStrategyBackPropagation" instance = "ExperFinderFactory::P1S" level="E"/>
		</objectInstanceLogLevel>

		<serviceLogLevel default="A">
		</serviceLogLevel>

		<customLogLevel default="N">
			<class name="AISockServer::debug" level="A"/>
			<class name="AISockServer::chat" level="A"/>
		</customLogLevel>
	</logging>
</engine>
